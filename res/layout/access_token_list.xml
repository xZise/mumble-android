<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical"
	android:layout_height="fill_parent"
	android:layout_width="fill_parent">
	<ListView android:layout_width="fill_parent"
		android:layout_height="fill_parent"
		android:id="@+id/accessTokenList"
		android:scrollbars="vertical"
		android:layout_weight="1"></ListView>
	<LinearLayout
		android:id="@+id/fooLinearLayout02"
		android:layout_height="wrap_content"
		android:layout_width="fill_parent">	
		<EditText
			android:layout_height="fill_parent"
			android:id="@+id/accessTokenName"
			android:imeOptions="actionSend"
			android:layout_width="wrap_content"
			android:enabled="false"
			android:layout_weight="1" android:hint="@string/accessTokenName"></EditText>
		<Button
			android:layout_width="wrap_content"
			android:layout_height="fill_parent"
			android:enabled="false"
			android:id="@+id/accessAddToken" android:text="@string/accessAddToken"></Button>
	</LinearLayout>

</LinearLayout>
